<h2 class= "container"> New Student </h2>



<%= form_with model: @student do |form| %>
    <p> <strong> Name : </strong> <%= form.text_field :name %> </p>
    <p> <strong> roll : </strong> <%= form.text_field :roll %> </p>
    <p> <strong> email : </strong> <%= form.text_field :email %> </p>
    <p> <strong> docs : </strong> <%= form.file_field :docs %> </p>
    <%= form.submit %>
    <% end %> 